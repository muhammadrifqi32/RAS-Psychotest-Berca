    </script>

    <style>
        #nilaiiq {
            border-top: 1px solid #000;
            border-spacing: 10px;
            width: 80%;
            height: 310px;
        }

            #nilaiiq tr th {
                border-top: 2px solid #5CCCDD;
                border-bottom: 2px solid #5CCCDD;
            }

            #nilaiiq tr td {
                border-bottom: 1px solid #000;
            }

        #nilaitotaliq {
            border-top: 1px solid #000;
            border-spacing: 20px;
            width: 80%;
            height: 40px;
        }

            #nilaitotaliq tr th {
                border-top: 2px solid #5CCCDD;
                border-bottom: 2px solid #5CCCDD;
            }

        #indexiq {
            border-top: 1px solid #000;
            border-spacing: 10px;
            width: 100%;
            height: 310px;
            text-align: center;
        }

            #indexiq tr th {
                border-top: 2px solid #5CCCDD;
                border-bottom: 2px solid #5CCCDD;
            }

            #indexiq tr td {
                border-bottom: 1px solid #000;
            }

        #downloadPage, #resetBut {
            display: block;
            width: 100%;
            border: none;
            color: white;
            height: 5vh;
            border-radius: 10px;
            font-size: 15px;
            font-weight: bold;
            text-transform: uppercase;
        }
    </style>
    @RenderSection("style",required:false)



</head>
<body class="hold-transition sidebar-mini layout-fixed" style="text-rendering: optimizeLegibility;">
    <div class="wrapper" id="forPDF" style="margin-left: 0; ">

        <!-- Preloader -->
        <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="~/AdminLTE-3.2.0/dist/img/logo-berca.png" alt="AdminLTELogo" height="60" width="60">
        </div>

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-dark sticky-top" style="margin-left: 0;">
            <!-- navbar-white navbar-light -->
            <!-- Logo -->
            <a class="navbar-brand" href="#" style="margin-left: 20px;">
                <img src="~/Image/berca-white.png" alt="Berca" style="width: auto; height: 40px;">
            </a>
        </nav>

        <div class="content-wrapper" style="margin-left: 0px; margin-right: 0px;">
            <br />

            <!-- Konten Anda -->
            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper" style="margin-left: 0; ">
                <!-- Content -->
                <div class="content-wrapper" style="margin-left: 25px; margin-right: 25px;">

                    <!-- Card Pertama -->
                    <div class="card" id="outCard" style=" margin-bottom: 10px;box-shadow:none;">
                        <div class="card" style="margin-bottom: 10px ;box-shadow:none; ">
                            <div class="card-header" style="border: none;">
                                <!-- Menghapus border kolom -->
                                <div class="row">
                                    <!-- Kolom Kiri untuk Logo -->
                                    <div class="col-md-10">
                                        <!-- Menghapus card di dalamnya -->
                                        <div class="text-left">
                                            <!-- Menggunakan text-left untuk meratakan teks ke kiri -->
                                            <img src="~/AdminLTE-3.2.0/dist/img/logo_Bhp bayang.png" alt="Berca" style="width: auto; height: 40px;">
                                        </div>
                                    </div>
                                    <!-- Kolom Kanan untuk Teks -->
                                    <div class="col-md-2 text-center" style="display: flex; flex-direction: column; justify-content: center;">
                                        <!-- Menggunakan text-right untuk meratakan teks ke kanan -->
                                        <div class="bg-red p-1 rounded text-white" id="secret" style="font-size:15px;">
                                            <!-- Menambahkan background merah dan border merah -->
                                            <!-- Menggunakan text-center untuk mengatur teks ke tengah -->
                                            <p class="card-text font-weight-bold">Rahasia<br />Hanya untuk psikolog</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        @RenderBody()

                    </div>

                </div>
            </div>
        </div>

        <!-- /.content-wrapper -->
        <footer class="main-footer" style="margin-left: 0;">
            <strong>Copyright &copy; 2023 <a href="https://adminlte.io">Psychotest PT Berca Hardayaperkasa</a>.</strong>
            All rights reserved.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 3.2.0
            </div>
        </footer>


    </div>
