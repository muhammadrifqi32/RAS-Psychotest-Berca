@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Admin/Participant/Result/Layout/Layout.cshtml";
}
@section style{
    <style>

    </style>
    }

<div class="card-body">

    <div class="row">
        <div class="col-md-12">
            <div class="card my-column" style="height: 50%; background-color: #f2f2f2;">
                <div class="card-body text-center" style="display: flex; flex-direction: column; justify-content: center;">
                    <!-- Menggunakan text-center untuk mengatur teks ke tengah -->
                    <h2 class="card-title"> HASIL TES KEPRIBADIAN (DISC) </h2>
                </div>
            </div>
        </div>
    </div>

    <!-- Untuk DAta personal dan screenshot-->

    <div class="row">
        <div class="col-md-4">
            <div class="card">
                <div class="card-body">
                    <table class="table dt-responsive nowrap table-sm table-striped" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <tr>
                            <th>Nama</th>
                            <td id="Nama">John Doe</td>
                        </tr>
                        <tr>
                            <th>Email</th>
                            <td id="Email">johndoe@example.com</td>
                        </tr>
                        <tr>
                            <th>NIK</th>
                            <td id="NIK">1234567890</td>
                        </tr>

                    </table>
                </div>

            </div>
        </div>
        <div class="col-md-4">
            <div class="card">
                <div class="card-body">
                    <table class="table dt-responsive nowrap table-sm table-striped" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <tr>
                            <th>Usia</th>
                            <td id="usia"></td>
                        </tr>
                        <tr>
                            <th>Jenis Kelamin</th>
                            <td id="gender"></td>
                        </tr>
                        <tr>
                            <th>Telepon</th>
                            <td id="Telp"></td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="card">
                <div class="card-header text-center" style="background: #f2f2f2;  margin: 0; height: 30px;  display: flex; align-items: center; justify-content: center;"> Pengambilan Gambar Wajah </div>
                <div class="card-body" style="max-height: 150px; overflow: auto;display: flex; align-items: center; justify-content: center;">
                    @*<img src="/image/facecapturing/qwewefwv-1693369859484.jpg" alt="Screenshot" style="height: 75px; border-radius: 5%; text-align: center;">*@
                    <img id="Screenshot" alt="Screenshot" style="height: 75px; border-radius: 5%; text-align: center;">
                </div>
            </div>
        </div>
    </div>

    <div class="row" style="display:none;" id="violation">
        <div class="col-md-12">
            <div class="card" style="box-shadow:none;">
                <div class="card-header text-center" style="background: #8B0000; margin: 0; color: white; height: 60px; padding: 5px;  width: 100%;  display: flex; align-items: center; justify-content: center;">
                    <div class="card-body text-center" style="display: flex; flex-direction: column; justify-content: center;">
                        <h2 id="headerfinalscore" class="card-title" style="font-size: 18px;"><strong><i class="fas fa-exclamation-triangle"></i> VIOLATION</strong><br /> Peserta terdeteksi membuka tab atau window baru atau aplikasi lain sebanyak <span id="totalviolation"></span> kali saat tes berlangsung.</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row" id="invalid">
        
    </div>

    <div class="row" style="display:none;" id="safe">
        <div class="col-md-12">
            <div class="card" style="box-shadow:none;">
                <div class="card-header text-center" style="background: #005B41; margin: 0; color: white; height:70px; padding: 5px;  width: 100%;  display: flex; align-items: center; justify-content: center;">
                    <div class="card-body text-center" style="display: flex; flex-direction: column; justify-content: center;">
                        <i class="fa-solid fa-triangle-exclamation"></i>
                        <h2 id="headerfinalscore" class="card-title" style="font-size: 18px;"><strong> SAFE</strong><br />Peserta mengikuti tes dengan baik dan benar.</h2>

                    </div>
                </div>
            </div>

        </div>
    </div>

    <div class="row" id="valueDisc" style="display:none;">

        <div class="col-md-12">
            <div class="card my-column" style="height: 85%;">
                <div class="card-body text-center" style="display: flex; flex-direction: column; justify-content: center;">
                    <!-- Menggunakan text-center untuk mengatur teks ke tengah -->
                    @*<h3 class="card-title"> Tes DISC </h3>*@

                    <!-- Tabel DataTables -->
                    <table id="discTable" class="table table-striped">
                        <thead>
                            <tr>
                                <th></th> <!-- Kolom kosong untuk label kolom pertama -->
                                <th>D</th>
                                <th>I</th>
                                <th>S</th>
                                <th>C</th>
                                <th>*</th>
                                <th>Total</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Most (M)</td>
                                <td id="data1">Data D Kolom 2</td>
                                <td id="data2">Data I Kolom 3</td>
                                <td id="data3">Data S Kolom 4</td>
                                <td id="data4">Data C Kolom 5</td>
                                <td id="data5">Data * Kolom 6</td>
                                <td id="totalmost">Data Score Kolom 7</td>
                            </tr>
                            <tr>
                                <td>Least (L)</td>
                                <td id="data6">Data D Kolom 2</td>
                                <td id="data7">Data I Kolom 3</td>
                                <td id="data8">Data S Kolom 4</td>
                                <td id="data9">Data C Kolom 5</td>
                                <td id="data10">Data * Kolom 6</td>
                                <td id="totalleast">Data Score Kolom 7</td>
                            </tr>
                            <tr>
                                <td><strong>Change</strong></td>
                                <td id="data11">Data D Kolom 2</td>
                                <td id="data12">Data I Kolom 3</td>
                                <td id="data13">Data S Kolom 4</td>
                                <td id="data14">Data C Kolom 5</td>
                                <td id="data15">Data * Kolom 6</td>
                                <td id="totalchange">Data Score Kolom 7</td>
                            </tr>
                            <!-- Tambahkan baris data lainnya sesuai dengan kebutuhan -->
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

        <div class="col-md-4">
            <div class="card">
                <div class="card-header text-center" style="background: #5e6ee6; margin: 0; color: white; height: 50px; padding: 5px;  width: 100%;  display: flex; align-items: center; justify-content: center;">
                    <h2 id="Graph1Title" class="card-title" style="font-size: 18px;"><strong>Graph 1</strong></h2>
                </div>
                <div class="card-body">
                    <div id="Graph1">
                        <canvas id="Graph1Chart" style="height: 500px; display: block; width: 585px;"></canvas>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-4">
            <div class="card">
                <div class="card-header text-center" style="background: #5e6ee6; margin: 0; color: white; height: 50px; padding: 5px;  width: 100%;  display: flex; align-items: center; justify-content: center;">
                    <h2 id="Graph2Title" class="card-title" style="font-size: 18px;"><strong>Graph 2</strong></h2>
                </div>
                <div class="card-body">
                    <div id="Graph2">
                        <canvas id="Graph2Chart" style="height: 500px; display: block; width: 585px;"></canvas>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-4">
            <div class="card">
                <div class="card-header text-center" style="background: #5e6ee6; margin: 0; color: white; height: 50px; padding: 5px;  width: 100%;  display: flex; align-items: center; justify-content: center;">
                    <h2 id="Graph3Title" class="card-title" style="font-size: 18px;"><strong>Graph 3</strong></h2>
                </div>
                <div class="card-body">
                    <div id="Graph3">
                        <canvas id="Graph3Chart" style="height: 500px; display: block; width: 585px;"></canvas>
                    </div>
                </div>
            </div>
        </div>


        <div class="col-md-12">
            <div class="card" style="height: 275px; box-shadow:none;">
                <div class="card-header text-center" style="background: #5e6ee6; margin: 0; color: white; height: 60px; padding: 1px;  width: 100%;  display: flex; align-items: center; justify-content: center;">
                    <h2 id="Finalscore" class="card-title" style="font-size: 18px;"><strong></strong></h2>
                </div>

                <div class="card-body">
                    <div id="desdisc" style="text-align: justify;">
                    </div>


                </div>

            </div>

        </div>



    </div>

    <div class="row" id="forButton">
        <div class="col-12">
            <button class="bg-primary" id="downloadPage">download hasil tes</button>
            </div>
    </div>

</div>

